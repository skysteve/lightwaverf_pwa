{
  "extends": "airbnb/base",
  "rules" : {
    "comma-dangle" : 0,
    "object-shorthand" : 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "class-methods-use-this": 0
  },
  "env": {
    "browser": true
  }
}
